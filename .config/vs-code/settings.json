{
	"window.zoomLevel": 0,
	"window.nativeTabs": true,
	"window.newWindowDimensions": "maximized",

	"window.openFoldersInNewWindow": "off",
	"window.openFilesInNewWindow": "off",

	"telemetry.enableTelemetry": false,
	"telemetry.enableCrashReporter": false,

	"workbench.tips.enabled": false,
	"workbench.settings.editor": "json",
	"workbench.view.alwaysShowHeaderActions": true,
	"workbench.editor.highlightModifiedTabs": true,
	"workbench.startupEditor": "newUntitledFile",

	"workbench.colorTheme": "Plastic",
	"workbench.colorCustomizations": {
		"[Plastic]": {
			"editor.background": "#030405",
			"editor.lineHighlightBackground": "#000",
			"editor.lineHighlightBorder": "#0c1013",
			"editorWhitespace.foreground": "#131313",
			"editorIndentGuide.background": "#131313"
		}
	},
	"explorer.compactFolders": false,

	"editor.fontSize": 16,
	"editor.lineHeight": 18,
	"editor.fontWeight": "300",
	"editor.fontLigatures": true,
	"editor.fontFamily": "'Operator Mono', OperatorMonoNerdFontC-Book",

	"editor.tokenColorCustomizations": {
		"[Plastic]": {
			"textMateRules": [
				{
					"name": "Comments",
					"scope": [ "comment.block", "comment.line" ],
					"settings": {
						"fontStyle": "",
						"foreground": "#747474"
					}
				},
				{
					"name": "docBlock Comments",
					"scope": [ "comment.block.documentation" ],
					"settings": {
						"fontStyle": "",
						"foreground": "#7e7e7e"
					}
				},
				{
					"name": "Operator Mono Italics",
					"scope": [ "storage.type.php", "keyword.other.new.php", "variable.language.this.php", "keyword.control.exception.catch.php", "keyword.operator.return-value.php", "punctuation.definition.storage-type.begin.bracket.round.php", "punctuation.definition.storage-type.end.bracket.round.php" ],
					"settings": {
						"fontStyle": "italic"
					}
				}
			]
		}
	},

	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.detectIndentation": true,
	"editor.autoIndent": "full",

	"editor.renderWhitespace": "boundary",
	"editor.renderControlCharacters": false,
	"editor.renderIndentGuides": true,

	"editor.autoSurround": "never",
	"editor.autoClosingQuotes": "never",
	"editor.autoClosingBrackets": "beforeWhitespace",

	"spaces-inside-braces.enable": true,
	"spaces-inside-braces.enableForAngle": true,
	"spaces-inside-braces.enableForBraces": true,
	"spaces-inside-braces.enableForBrackets": true,
	"spaces-inside-braces.enableForParens": true,

	"editor.acceptSuggestionOnEnter": "smart",
	"editor.suggest.snippetsPreventQuickSuggestions": true,
	"editor.suggestOnTriggerCharacters": false,
	"editor.parameterHints.enabled": false,
	"editor.suggest.localityBonus": true,
	"editor.quickSuggestionsDelay": 1000,
	"editor.hover.delay": 1000,
	"editor.suggestFontSize": 15,
	"editor.suggestLineHeight": 16,
	"editor.quickSuggestions": {
		"other": true,
		"comments": true,
		"strings": true
	},

	"editor.formatOnType": false,
	"editor.formatOnPaste": false,
	"editor.formatOnSave": false,
	"editor.formatOnSaveTimeout": 5000,

	"editor.minimap.enabled": false,
	"editor.minimap.showSlider": "always",

	"editor.wordWrap": "off",
	"editor.wordWrapColumn": 120,
	"editor.wrappingIndent": "indent",

	"editor.folding": false,
	"editor.scrollBeyondLastLine": false,

