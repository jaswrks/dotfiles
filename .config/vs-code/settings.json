	"files.eol": "\n",
	"files.encoding": "utf8",

	"files.autoSave": "off",
	"files.autoSaveDelay": 1000,
	"files.enableTrash": true,

	"files.exclude": {
		"**/.git": true,
		"**/.git-dir": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/Icon?": true,
		"**/.DS_Store": true,
		"**/.linaria-cache": true
	},
	"files.associations": {
		"*.php-x": "php"
	},
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"files.trimFinalNewlines": true,

	"search.exclude": {
		"**/vendor": true,
		"**/node_modules": true,
		"**/jspm_packages": true,
		"**/bower_components": true
	},
	"search.useIgnoreFiles": true,
	"search.useGlobalIgnoreFiles": true,
	"search.collapseResults": "alwaysCollapse",

	"git.fetchOnPull": true,
	"git.rebaseWhenSync": true,
	"git.allowForcePush": true,
	"git.enableCommitSigning": true,
	"git.showPushSuccessNotification": true,
	"git.promptToSaveFilesBeforeCommit": "always",
	"git.path": "/usr/local/bin/git",

	"eslint.run": "onSave",
	"eslint.format.enable": true,
	"eslint.lintTask.enable": true,
	"eslint.nodePath": "/usr/local/lib/node_modules",
	"eslint.options": {
		"configFile": "/users/jaswrks/.eslintrc.json"
	},

	"dartsass.disableSourceMap": true,
	"dartsass.disableAutoPrefixer": false,
	"dartsass.disableCompileOnSave": true,
	"dartsass.enableStartWithUnderscores": false,
	"dartsass.disableMinifiedFileGeneration": false,
	"dartsass.sassBinPath": "/usr/local/bin/sass",
	"dartsass.autoPrefixBrowsersList": [ ">= 0.2%", "not ie <= 11", "not op_mini all", "not dead" ],

	"prettier.requireConfig": true,
	"prettier.prettierPath": "/usr/local/lib/node_modules/prettier",
	"prettier.configPath": "/users/jaswrks/.prettierrc.json",
	"prettier.ignorePath": ".prettierignore",

	"php.suggest.basic": true,
	"php.validate.enable": true,
	"php.validate.run": "onSave",
	"php.validate.executablePath": "/usr/local/bin/php",

	"phpcs.enable": true,
	"phpcs.standard": null,
	"phpcs.autoConfigSearch": true,
	"phpcs.executablePath": "/users/jaswrks/.composer/vendor/bin/phpcs",
	"phpcs.showSources": true,

	"phpcbf.enable": true,
	"phpcbf.standard": null,
	"phpcbf.documentFormattingProvider": true,
	"phpcbf.executablePath": "/users/jaswrks/.composer/vendor/bin/phpcbf",

	"shellcheck.enable": true,
	"shellcheck.run": "onSave",
	"shellcheck.exclude": [ "1090", "2155", "2114", "1091", "2129" ],
	"shellcheck.executablePath": "/usr/local/bin/shellcheck",

	"terminal.integrated.fontSize": 15,
